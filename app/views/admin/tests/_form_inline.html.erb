<%= form_with model: [:admin, test], url: update_inline_admin_test_path(test), local: true, class: "form-inline" do |form| %>
  <div class="row">
    <div class="col-6">
      <%= form.text_field :title, class: "form-control mb-3" %>
    </div>
    <div class="col-4">  
      <%= form.submit t(".submit"), class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
