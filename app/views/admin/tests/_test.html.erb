<tr >

  <td class="align-middle">
    <%= test.title %>
    <%= render "form_inline", test: test == @test ? @test : test %>
  </td>
  <td class="align-middle"><%= test.category.title %></td>
  <td class="align-middle"><%= test_level(test) %></td>
  <td class="align-middle"><%= test.questions.count %></td>

  <td class="align-middle">
    <%= link_to t("show"), admin_test_path(test), class: "btn btn-success" %>
  </td>
  <td class="align-middle">
    <%= link_to t("edit"), edit_admin_test_path(test), class: "btn btn-warning" %>
  </td>
  <td class="align-middle">
    <%= link_to t("delete"), admin_test_path(test), class: "btn btn-danger", method: :delete, data: { confirm: t("confirmation") } %>
  </td>

</tr>
